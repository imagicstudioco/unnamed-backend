{
  "name": "fundraiser-backend",
  "version": "1.0.0",
  "description": "Backend for fundraising platform",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias && npm run copy-files",
    "copy-files": "cp -r ./src/templates ./dist/",
    "dev": "nodemon --trace-warnings",
    "start": "node ./dist/index.js",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cloudinary": "1.41.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "ethers": "6.11.1",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.6.8",
    "multer": "1.4.5-lts.1",
    "multer-storage-cloudinary": "4.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/multer": "^1.4.12",
    "@types/node": "18.15.11",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.7",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  }
}
